PSUADE
INPUT
   dimension = 2
   variable  1 X1 = 0      10
   variable  2 X2 = 0      10
   discrete  1
END
OUTPUT
   dimension = 1
   variable 1 Y
END
METHOD
   sampling = LH
   num_samples = 100
   random_seed = 41348131
END
APPLICATION
   driver = ./simulator4a
   opt_driver = ./simulator4a
END
ANALYSIS
   optimization method = sce
   optimization num_local_minima = 1
   optimization max_feval = 10000
#  optimization fmin = 0.0
   optimization tolerance = 1.000000e-04
   optimization num_fmin = 1
   optimization print_level = 1
   printlevel 2
END
END
