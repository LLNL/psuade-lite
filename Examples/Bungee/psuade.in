PSUADE
INPUT
   dimension = 3
   variable   1 H   = 40  60
   variable   2 M   = 67  74
   variable   3 sig = 20  40
END
OUTPUT
   dimension = 1
   variable 1 Y1
END
METHOD
   sampling = LPTAU
   num_samples = 300
   randomize
   random_seed = 41491431
END
APPLICATION
   driver = ./simulator
#  if ensemble mode is desired, uncomment the next 3 lines
#  ensemble_driver = ./ensemble_simulator
#  ensemble_run_mode
#  max_parallel_jobs = 300
END
ANALYSIS
   analyzer output_id  = 1
   printlevel 1
END
END
