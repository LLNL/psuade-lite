   optimization num_local_minima = 1
   optimization max_feval = 10000
#  optimization fmin = 1.000000e-02
   optimization tolerance = 1.000000e-06
#  optimization num_fmin = 2
#  optimization print_level = 3
   printlevel 3
END
END
