PSUADE
INPUT
   dimension = 2
   variable 1 X1 =  0   1
   variable 2 X2 =  0   1
END
OUTPUT
   dimension = 1
   variable 1 Y1
END
METHOD
   sampling = LPTAU
   num_samples = 1
#  randomize
END
APPLICATION
   driver = ./simulator
   opt_driver = ./simulator
END
ANALYSIS
